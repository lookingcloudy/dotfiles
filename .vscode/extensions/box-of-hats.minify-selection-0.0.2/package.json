{
	"name": "minify-selection",
	"displayName": "Minify selection",
	"description": "Minify just your selection",
	"version": "0.0.2",
	"author": {
		"name": "box-of-hats",
		"url": "https://github.com/Box-Of-Hats",
		"email": "JLBerg@hotmail.co.uk"
	},
	"publisher": "box-of-hats",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"icon": "img/icon.png",
	"repository": {
		"url": "https://github.com/Box-Of-Hats/vscode-minify-selection.git"
	},
	"activationEvents": [
		"onCommand:extension.minifySelection.minifySelection",
		"onLanguage:css",
		"onLanguage:html"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.minifySelection.minifySelectedCSS",
				"title": "Minify Selection: Minify Selected CSS",
				"enablement": "editorLangId == css || editorLangId == html"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"clean-css": "^4.2.1",
		"html-minifier": "^4.0.0",
		"uglify-js": "^3.6.0"
	},
	"__metadata": {
		"id": "55e378d8-c5b7-48f1-aac9-ed4c6b716528",
		"publisherId": "dd591033-3885-4c57-b7d0-858492ae93b2",
		"publisherDisplayName": "Box-Of-Hats",
		"size": 2105448
	}
}